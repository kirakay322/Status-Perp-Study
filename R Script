eta_squared(car::Anova(RQExp_Y3_3, type = 3))

#Y4: Personality
RQExp_Y4_3 <- lm(data = d3, PersonalityP_av ~ ESJ_av*Atonement_fact_r*SES, contrasts =
                   my.contrasts)
